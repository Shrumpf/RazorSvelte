{
  "name": "svelterazoraspnet",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dotnet-run": "dotnet run",
    "dotnet-clean": "dotnet clean",
    "dotnet-build": "dotnet build",
    "scss-build": "node-sass ./Styles/style.scss ./wwwroot/style.css --output-style compressed",
    "scss-watch": "node-sass ./Styles/style.scss ./wwwroot/style.css --output-style compressed --watch --recursive",
    "index-build": "rollup -c ./Pages/Index/rollup.config.js",
    "index-watch": "rollup -c ./Pages/Index/rollup.config.js -w",
    "privacy-build": "rollup -c ./Pages/Privacy/rollup.config.js",
    
    "upgrade-npms": "ncu --upgrade && npm install"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "rollup": "^2.55.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.41.0",
    "svelte-check": "^2.2.3",
    "svelte-preprocess": "^4.7.4",
    "typescript": "^4.3.5",
    "tslib": "^2.3.0",
    "@tsconfig/svelte": "^2.0.1",
    "node-sass": "^6.0.1",
    "sass": "^1.37.0",
    "bootstrap": "^5.0.2",
    "@popperjs/core": "^2.9.2"
  },
  "dependencies": {}
}
