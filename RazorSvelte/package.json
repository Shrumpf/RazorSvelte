    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.38.2",
    "svelte-check": "^1.5.4",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.2.4",
    "tslib": "^2.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "node-sass": "^5.0.0",
    "sass": "^1.32.11",
    "bootstrap": "^5.0.1",
    "@popperjs/core": "^2.9.2"
  },
